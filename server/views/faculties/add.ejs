<% include ../partials/header.ejs %>

<!-- Content  for the add page -->

    <div class="container">
      <div class="row">
        <div class="offset-md-3 col-md-6">
          <h1><%=title %></h1>
    
          <form class="form" method="post">
            <div class="from-group">
              <label for="facultyidTextField">Facultyid</label>
              <input
                type="text"
                class="form-control"
                id="facultyid"
                placeholder="Enter the Facultyid:"
                name="Facultyid"
                required
              />
            </div>
    
            <div class="from-group">
              <label for="authorTextField">Facultyname</label>
              <input
                type="text"
                class="form-control"
                id="FacultynameTextField"
                placeholder="Enter the Facultyname:"
                name="Facultyname"
                required
              />
            </div>
    
            <div class="from-group">
              <label for="DepartmentTextField">Department</label>
              <input
                type="text"
                class="form-control"
                id="DepartmentTextField"
                placeholder="Enter the Department:"
                name="Department"
                required
              />
            </div>
    
            <div class="from-group">
              <label for="SubjectTextField">Subject</label>
              <input
                type="text"
                class="form-control"
                id="SubjectTextField"
                placeholder="Enter the Subject:"
                name="Subject"
                required
              />
            </div>
            
            <button type="submit" class="btn btn-primary">
                <i class="fa fa-check-square-o"></i> Submit
              </button>
              <a href="/faculties" class="btn btn-warning"
                ><i class="fa fa-undo"></i> Cancel</a>
          </form>
        </div>
      </div>
      
    </div>

<% include ../partials/footer.ejs %>